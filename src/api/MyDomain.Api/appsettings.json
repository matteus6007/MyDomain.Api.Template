{
  "Serilog": {
    "Using": [
      "Serilog.Sinks.Console"
    ],
    "MinimumLevel": {
      "Default": "Information",
      "Override": {
        "Microsoft": "Warning",
        "Microsoft.AspNetCore": "Error",
        "System": "Warning"
      }
    },
    "WriteTo": [
      {
        "Name": "Console"
      }
    ],
    "Enrich": [
      "FromLogContext"
    ]
  },
  "AllowedHosts": "*",
  "MyDomain": {
    "Database": {
      "Server": "localhost",
      "ReplicaServer": "",
      "Port": 30000,
      "Schema": "MyDomainSchema"
    },
    "Sns": {
      "TopicName": "mydomain-api",
      "RetryCount": 3
    },
    "Secrets": {
      "SecretsKey": "mydomain-api"
    }
  },
  "AWS": {
    "Region": "eu-west-1"
  },
  "HealthChecksUI_Enabled": true
}