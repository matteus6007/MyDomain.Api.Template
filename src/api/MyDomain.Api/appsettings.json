{
  "Serilog": {
    "Using": ["Serilog.Sinks.Console"],
    "MinimumLevel": {
      "Default": "Information",
      "Override": {
        "Microsoft": "Warning",
        "Microsoft.AspNetCore": "Error",
        "System": "Warning"
      }
    },
    "WriteTo": [
      { "Name": "Console" }
    ],
    "Enrich": ["FromLogContext"]
  },
  "AllowedHosts": "*",
  "MyDomain": {
    "Database": {
      "Server": "localhost",
      "ReplicaServer": "",
      "Port": 30000,
      "Schema": "MyDomainSchema",
      "User": "root",
      "Password": "password123"      
    }
  }
}
